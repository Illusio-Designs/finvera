{
  "name": "app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "update-ip": "node scripts/update-ip.js",
    "env:dev": "node scripts/setup-env.js set development",
    "env:prod": "node scripts/setup-env.js set production",
    "env:current": "node scripts/setup-env.js current",
    "build:android": "npm run env:prod && eas build --platform android",
    "build:ios": "npm run:prod && eas build --platform ios",
    "build:dev": "npm run env:dev && expo export",
    "prebuild": "npm run env:dev"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.10.1",
    "axios": "^1.13.2",
    "babel-preset-expo": "^54.0.10",
    "expo": "~54.0.33",
    "expo-av": "~16.0.8",
    "expo-barcode-scanner": "~14.0.1",
    "expo-calendar": "~15.0.8",
    "expo-camera": "~17.0.10",
    "expo-clipboard": "~8.0.8",
    "expo-contacts": "~15.0.11",
    "expo-document-picker": "~14.0.8",
    "expo-file-system": "~19.0.21",
    "expo-font": "^14.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-intent-launcher": "~13.0.8",
    "expo-linear-gradient": "~15.0.1",
    "expo-linking": "~8.0.11",
    "expo-local-authentication": "~17.0.8",
    "expo-location": "~19.0.8",
    "expo-media-library": "~18.2.1",
    "expo-network": "^8.0.8",
    "expo-notifications": "~0.32.16",
    "expo-print": "~15.0.8",
    "expo-sharing": "~14.0.8",
    "expo-status-bar": "~3.0.9",
    "nativewind": "^4.2.1",
    "prop-types": "^15.8.1",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-qrcode-svg": "^6.3.21",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0"
  },
  "private": true,
  "devDependencies": {
    "@testing-library/react-native": "^12.4.3",
    "jest": "^29.7.0",
    "react-test-renderer": "19.1.0",
    "tailwindcss": "^3.4.19"
  }
}
